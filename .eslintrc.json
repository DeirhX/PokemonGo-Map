{
  "extends": "standard",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "define": true,
    "google": true,
    "mapData": true,
    "Store": true,
    "centerLat": true,
    "centerLng": true,
    "pageLoaded": true,
    "countMarkers": true,
    "skel": true,
    "setupPokemonMarker": true,

    "noLabelsStyle": true,
    "darkStyle": true,
    "light2Style": true,
    "pGoStyle": true,
    "darkStyleNoLabels": true,
    "light2StyleNoLabels": true,
    "pGoStyleNoLabels": true
  },
  "rules": {
    "semi": 0,
    "quotes": 0,
    "indent": ["warn", 4],
    "no-multiple-empty-lines": 0
  }
}
